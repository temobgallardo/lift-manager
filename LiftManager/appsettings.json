{
  "AppSettings": {
    "InitialFloor": 0,
    "NumberOfFloors": 5,
    "FilesForBulk": 10,
    "TravelSimulationTime": 500,
    "DatabaseDirectory": "${workspaceFolder}/LiftManager.db"
  },
  "Environment": "Dev",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "log.json",
          "rollingInterval": "Day",
          "MinimumLevel": "Information",
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ]
  }
}